    "class Employee():\n",
    "    def __init__(self, wage):\n",
    "        self.wage = wage\n",
    "\n",
    "    def weekly_pay(self, hours):\n",
    "        pay = self.wage * hours\n",
    "        return pay\n",
    "\n"
   ]
